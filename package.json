{
  "name": "lottoengine",
  "version": "1.0.0",
  "description": "This is the core engine designed to generate patterns and generate lines accordingly for playing lotto national lottery",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "prod:start": "pm2 start src/index.js --name 'lotto_core_engine'",
    "prod:stop": "pm2 stop 'lotto_core_engine'",
    "prod:monitor": "pm2 monit --name 'lotto_core_engine'",
    "prod:log": "pm2 log --name 'lotto_core_engine'",
    "create:db": "npx sequelize-cli db:create",
    "drop:db": "npx sequelize-cli db:drop",
    "migrate": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:load": "artillery run artillery/load-test.yaml"
  },
  "author": "sreerag",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0",
    "mathjs": "^12.4.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.4",
    "path": "^0.12.7",
    "pg": "^8.11.5",
    "pm2": "^5.3.1",
    "sequelize": "^6.37.2",
    "sequelize-cli": "^6.6.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.0"
  },
  "jest": {
    "verbose": true
  }
}
